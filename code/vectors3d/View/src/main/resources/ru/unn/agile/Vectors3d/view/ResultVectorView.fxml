<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="view" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.unn.agile.Vectors3d.view.ResultVectorView">
    <HBox AnchorPane.rightAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.topAnchor="0.0">
        <spacing>10</spacing>
        <children>
            <Label text="X:" />
            <Label fx:id="xLabel" prefWidth="150.0" HBox.hgrow="ALWAYS"/>
            <Label text="Y:" />
            <Label fx:id="yLabel" prefWidth="150.0" HBox.hgrow="ALWAYS"/>
            <Label text="Z:" />
            <Label fx:id="zLabel" prefWidth="150.0" HBox.hgrow="ALWAYS"/>
        </children>
    </HBox>
</AnchorPane>
